
	<h1>
	  <!--<strong>Title:</strong>-->
	  <%= @homework.title %>
	</h1>

	<h3>
	  <!--<strong>Question:</strong>-->
	  <%= @homework.question %>
	</h3>

  <h3>
  Username: <%=@user.username%>
  </h3>
	<br/>



<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Answer ID</th>
      <th>Answer</th>
    </tr>
  </thead>

  <tbody>
    <% @answers.each do |answer| %>
      <tr>
        <td><%=answer.id%></td>
        <td><%= answer.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>